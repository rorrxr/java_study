package chapter5.classpart2;

public class StudentTest {

	public static void main(String[] args) {
		// 
		Student st1 = new Student();
		
		/*
		 Student stu1;
		 stu1 = new Student; 
		 */
		
		
		// 메모리 사용
		// 3가지로 구분 : 스택영역, 힙영역, 메서드영역
		
		st1.studentID = 1; 
		st1.studentName = "홍길동";
		st1.grade = 3;
