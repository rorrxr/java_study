package chapter4.ifexample;

public class IfExample {
	public static void main(String[] args) {
		// 제어문 : if 구문
		
		boolean isMarried = true;
		
		// 조건식이 true이면 11번줄이 실행이 되고, false이면 13번 줄이 실행된다.
		if(isMarried) {
			System.out.println("기혼입니다.");
		}else {
			System.out.println("미혼입니다.");
		}
	}
}
