package chapter5.constructor;

// 개인정보를 관리하는 클래스
// Person 클래스가 컴파일 되면, 기본 생성자가 자동생성된다.
// Person 클래스 이름으로 만들어진 Person() {}
public class Person {
	
	String name;
	float height;
	float weight;
	
	// 컴파일 과정에서 자동으로 생성되는 생성자
	// 힙 영역에 생성된 기억장소를 기본 세팅해주는 작업.
	public Person() {
		
	}
	
	
	 public Person(String pname) { // 리턴 타입이란 것이 없다.
		name = pname;
	}
	 
	
}
